<p id="notice"><%= notice %></p>

<h1>Admin Faculties</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_faculties.each do |admin_faculty| %>
      <tr>
        <td><%= link_to 'Show', admin_faculty %></td>
        <td><%= link_to 'Edit', edit_admin_faculty_path(admin_faculty) %></td>
        <td><%= link_to 'Destroy', admin_faculty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Faculty', new_admin_faculty_path %>
